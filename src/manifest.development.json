{
    "manifest_version": 2,
    "name": "Quickly open multiple URLs",
    "description": "chrome extension",
    "version": "1.0.0",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}